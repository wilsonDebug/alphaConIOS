{
  "expo": {
    "name": "MobileAppUnified",
    "slug": "mobileappunified",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mobileappunified.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mobileappunified.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "name": "MobileAppUnified",
  "displayName": "MobileAppUnified"
}
